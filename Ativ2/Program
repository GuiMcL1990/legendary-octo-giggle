package Davidson_Tarde_Ativ_2.Main.Program2;

import Davidson_Tarde_Ativ_2.Main.Object2.Object2;

import java.util.Scanner;

public class Program {
        public static void main(String[] args) {
            int a, b;
            Scanner sc = new Scanner(System.in);

            System.out.println("Digite um numero:");
            a = sc.nextInt();
            System.out.println("Digite um numero:");
            b = sc.nextInt();

            Object2 maiorNum = new Object2(a,b);

            System.out.println(maiorNum.toString());

            sc.close();
        }
    }
