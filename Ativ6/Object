import java.util.Scanner;

class NumberProcessor {
    public int a, b, c;

    public NumberProcessor(int a, int b, int c) {
        this.a = a;
        this.b = b;
        this.c = c;
    }

    public int max() {
        if (a > b && a > c) {
            return a;
        } else if (b > c) {
            return b;
        } else {
            return c;
        }
    }

    public void showResult() {
        System.out.println("Higher = " + max());
    }
}
