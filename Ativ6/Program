public class Program {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter three numbers: ");
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
        
        NumberProcessor processor = new NumberProcessor(a, b, c);
        processor.showResult();
        
        sc.close();
    }
}
